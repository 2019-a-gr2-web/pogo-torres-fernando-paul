<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
<style>
    table, tr,td, th{
        border: solid;
    }
</style>
<a href="/api/traguito/crear">
    <button>Nuevo</button>
</a>

<table>
    <tr>
        <th>Nombre</th>
        <th>Grados</th>
        <th>Caduca</th>
        <th>Precio</th>
        <th>Tipo</th>
        <th>Acciones</th>
    </tr>
    <% arregloTragos.forEach((trago)=>{ %>
    <tr>
        <td> <%=trago.nombre%> </td>
        <td> <%=trago.gradosAlcohol%> </td>
        <td> <%=trago.fechaCaducidad%> </td>
        <td> <%=trago.precio%> </td>
        <td> <%=trago.tipo%> </td>
        <td>
            <button>Editar</button>
            <form action="/api/traguito/eliminar"
                  method="post"
            >
                <input type="number"  name="id" value="<%trago.id%>" hidden>
                <button>Eliminar</button>
            </form>
            <button>Eliminar</button>
        </td>
    </tr>
    <% })%>
</table>
</body>
</html>